{
  "expo": {
    "name": "Interval Timer",
    "description": "Interval Timer by Semicircle Labs",
    "slug": "Interval-Timer",
    "version": "1",
    "orientation": "portrait",
    "icon": "./assets/placeholder-app-icon.png",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/audio/*",
      "assets/screen-icons/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.semicirclelabs.intervaltimer",
      "config": {
        "usesNonExemptEncryption": false
     },
     "appStoreUrl": "https://apps.apple.com/us/app/memoir-breathing/id1544869710"
    },
    "android": {
      "package": "com.semicirclelabs.intervaltimer",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/placeholder-app-icon.png",
        "backgroundColor": "#000000"
      },
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.memoir.memoir"
    },
    "web": {
      "favicon": "./assets/placeholder-app-icon.png"
    }
  }
}
